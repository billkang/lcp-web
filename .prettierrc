{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": false,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf"
}
